@model ML.Usuario
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
 @using (Html.BeginForm("GetAll", "Registro", FormMethod.Get, new { enctype = "multipart/form-data" }))
        {
<div class="col" style="display:none">
       @Html.LabelFor(model => model.IdUsuario, new { @class = "visually-hidden" })

       @Html.TextBoxFor(model => model.IdUsuario, new { @class = "visually-hidden form-control" })
                </div>
<div class="col" style="display:none">
    @Html.LabelFor(model => model.Username, new { @class = "visually-hidden" })

    @Html.TextBoxFor(model => model.Username, new { @class = "visually-hidden form-control" })
</div>

<div class="col" style="display:none">
    @Html.LabelFor(model => model.Password, new { @class = "visually-hidden" })

    @Html.TextBoxFor(model => model.Password, new { @class = "visually-hidden form-control" })

       
</div>

    <input type="submit" value="Calcula Super Digito" class="btn btn-success" />
}